{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
      }
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "<your host:port>,
        "Certificate": {
          "Path": "<path to ssl>",
          "Password": "<your pass>"
        }
      }
    }
  },
  "Host": {
    "URL": "<App URL>",
    "RegistrationConfirmationPath": "<Path to registration confirmation endpoint>",
    "ConfirmationTokenPath": "<Path to confirmation endpoint>"
  },
  "JWT": {
    "Issuer": "",
    "Audience": "",
    "Expires": 0, //in minutes
    "Key": ""
  },
  "RefreshToken" : {
    "Expires": 0, //in days
    "MaxSessions": 0
  },
  "GoogleAuth": {
    "ClientId": "<google client id>",
    "ClientSecret": "<google client secret>",
    "RedirectUri": "<your callback endpoint to process the code>",
    "Scope": "<your need scope>"
  },
  "Mail": {
    "Mail": "<ses identity registered mail id>",
    "Region": "<your smtp region>",
    "AwsAccessKeyId": "<iam access key id>",
    "AwsSecretAccessKey": "<iam secret access key>",
    "MaxAttemptsConfirmationResend": 0,
    "NextAttemptAvailableAfter": "00:00:00",  // Format: hh:mm:ss
    "MinIntervalBetweenAttempts": "00:00:00" // Format: hh:mm:ss
  },
  "TokenConfig": {
    "TTL": {
      "RegistrationConfirmation": "00:00:00", // Format: hh:mm:ss
      "EmailChangeOld": "00:00:00", // Format: hh:mm:ss
      "EmailChangeNew": "00:00:00", // Format: hh:mm:ss
      "PasswordReset": "00:00:00", // Format: hh:mm:ss
      "PasswordChange": "00:00:00", // Format: hh:mm:ss
      "UsernameChange": "00:00:00"  // Format: hh:mm:ss
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "server=YourServerAddress;database=DB;user=YourUser;password=YourPass"
    "RedisConnectionUrl": "<redis URL:Port>"
  }
}